\chapter{Clé de détermination des familles}

\begin{cle}{angiospermes}
\item \LigneCritereCle{Herbacée à tige ramifiée, à feuille simple, sessile, engainante à nervures parallèles ; racines adventives fasciculées ; fleurs de type 3}{\nom{clemonocotyledones}{}}
\item \LigneCritereCle{Herbacée ne présentant pas ces caractères ou plante ligneuse}{\nom{cleeudicotyledones}{}}
\end{cle}

\begin{cle}{monocotyledones}
\item \LigneCritereCle{Fleurs discrètes}{\nom{clefleursdiscretes}{}}
\item \LigneCritereCle{Fleurs colorées}{\nom{clefleurscolorees}{}}
\end{cle}

\begin{cle}{fleursdiscretes}
