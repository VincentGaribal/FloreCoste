\end{cle}

\begin{cle}{formulefloralesimplifie}
\item \LigneCritereCle{Tige pleine, feuilles à gaine non fendue et insérées sur trois rangs}{\nom{Cyperaceae}{Cyperaceae}}
\item \LigneCritereCle{Tige creuse, feuilles à gaine fendue et insérées sur trois rangs}{\nom{Poaceae}{Poaceae}}
\end{cle}

\begin{cle}{fleurscolorees}
